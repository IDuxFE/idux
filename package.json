    "postcss-less": "^5.0.0",
    "prettier": "^2.5.0",
    "prismjs": "^1.25.0",
    "remark": "^13.0.0",
    "resolve-bin": "^1.0.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.0",
    "stylelint-config-standard": "^24.0.0",
    "svgo": "^2.8.0",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.0",
    "unplugin-vue-components": "^0.17.0",
    "vite": "^2.7.0",
    "vue3-jest": "^27.0.0-alpha.2",
    "yaml-front-matter": "^4.1.1"
  },
  "engines": {
    "node": ">= 16.0.0"
  }
}
